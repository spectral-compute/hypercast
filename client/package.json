{
  "name": "live-video-streamer-client",
  "description": "Client for ultra low-latency live video streaming over CDN",
  "author": "Spectral Compute",
  "license": "Custom",
  "private": true,
  "main": "dist/live-video-streamer-client.js",
  "types": "dist/live-video-streamer-client.d.ts",
  "version": "0.0.0",
  "scripts": {
    "bundle": "(cd ../common && yarn tsc) && ts-patch install && yarn webpack --mode production \"$@\"",
    "bundle-dev": "(cd ../common && yarn tsc) && ts-patch install && yarn webpack --mode development \"$@\"",
    "clean": "rm -rf build dist",
    "lint": "yarn eslint --max-warnings=0 --ext .ts src test",
    "test": "ts-patch install && jest --config test/jest.config.js \"$@\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.18.0",
    "@babel/preset-env": "^7.17.10",
    "@ckitching/typescript-is": "^0.1.21",
    "babel-loader": "^8.2.5",
    "live-video-streamer-common": "workspace:^",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "ts-loader": "^9.3.0",
    "ts-patch": "^2.0.1",
    "typescript": "^4.6.4",
    "typescript-declaration-webpack-plugin": "^0.2.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "ts-jest": "^28.0.2"
  }
}
